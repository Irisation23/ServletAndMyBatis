<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.data.emp.EmployeesMapper">
  <select id="findAllEmployees" resultType="Emp">
    SELECT employee_id,
           first_name,
           last_name,
           email,
           phone_number,
           hire_date,
           salary,
           commission_pct,
           manager_id,
           department_id
    FROM EMPLOYEES;
  </select>
</mapper>

